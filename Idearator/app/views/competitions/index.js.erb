var main ;
if($('.topten').length == 0){
  main = $('.content');
}else{
  main = $('#Searchpreview');
}
<% if @firstTime %>
if(!($('.competition').length > 0)){
  main.empty();
  main.append("<%= escape_javascript(render(:partial => "index")) %>");
}
<% else %>
<% if @filter %>
main.remove();
<% end %>
main.append("<%= escape_javascript(render(:partial => "index")) %>");
apply_script();
<% end %>
