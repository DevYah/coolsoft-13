<div class='well notification approve-committee <%= notification.read_by?(current_user) ? 'read' : 'unread' %>' data-notification = <%= notification.user.id %> data-time=<%= notification.created_at.to_i %>>
  <span><%= notification.text %></span>
  <div class='link'><%= link_to 'review committee', notification.user.becomes(User)%></div>
  <span class='date'><%= notification.created_at.to_s(:long)%></span>
</div>