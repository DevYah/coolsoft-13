<div class='well notification delete <%= notification.read_by?(current_user) ? 'read' : 'unread' %>' data-time=<%= notification.created_at.to_i %> data-notification = <%= notification.id %>>
  <span><%= notification.text %></span>
  <span class='date'><%= notification.created_at.to_s(:long)%></span>
</div>
