$('div#idea--<%= @idea.id %>').remove();
var remaining = $('.btn-success.add-rating').length ;
if (remaining == 0){
  var z = $('<h1>No ideas to review</h1>');
  $('div.content').prepend(z);
}
var btns = $('div#approve-btns').length;
if (btns == 1){
  $('div#approve-btns').remove();
  $('div.add-ratings').remove();
}