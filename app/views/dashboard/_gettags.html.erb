<div id = "tagsall">	
<table class = "table table-bordered" id = "tabletags">
	<th>Tags</th>
<% @ideatags.each do |it| %>
<tr class = "success">
<td><i class="icon-tag"></i><%= link_to it.name, :controller=> "dashboard", :action => "getideas", :tagid => it.id, :remote => true %>
	</td>
</tr>
<% end %>
</table>
</div>
